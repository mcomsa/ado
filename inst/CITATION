year <- 2018
note <- paste0("R package version ", utils::packageVersion("ado"))

bibentry(bibtype = "Manual",
         title = "{ado}: An Implementation of Stata's Ado Language for R",
         author = c(person("William", "Brannon")),
         year = {year},
         note = note,
         url = "https://github.com/wwbrannon/ado")
